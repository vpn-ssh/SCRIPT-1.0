echo -e "\033[0;37m‚îÉ                                                   \033[0;37m‚îÉ"
echo -e "\033[0;37m‚îÉ\033[1;31m‚Ä¢ \033[1;37mINSTALAR UM CONJUNTO DE SCRIPTS COMO FERRAMENTAS\033[0m \033[0;37m‚îÉ"
echo -e "\033[0;37m‚îÉ\033[1;37m  PARA O GERENCIAMENTO DE REDE, SISTEMA E USUARIOS\033[0m \033[0;37m‚îÉ"
echo -e "\033[0;37m‚îÉ                                                   \033[0;37m‚îÉ"
echo -e "\033[0;37m‚îÉ\033[1;32m‚Ä¢ \033[1;32mDICA! \033[1;37mULTILIZE O TEMA DARK EM SEU TERMINAL PARA\033[0m  \033[0;37m‚îÉ"
echo -e "\033[0;37m‚îÉ\033[1;37m  UMA MELHOR EXPERI√äNCIA E VISUALIZACAO DO MESMO!\033[0m  \033[0;37m‚îÉ"
echo -e "\033[0;37m‚îÉ                                                   \033[0;37m‚îÉ"
echo -e "\033[0;37m‚î£‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚î´ \033[0m"
echo -e "\033[0;37m‚îÉ                \e[38;5;82mby:\e[38;5;226m@\e[38;5;198m@CoutySSH\033[1;37m       \033[0m         \033[0;37m‚îÉ"
echo -e "\033[0;37m‚îó‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îõ\033[0m"           
#-----------------------------------------------------------------------------------------------------------------
echo -ne "\033[1;36mDESEJA CONTINUAR [N/S]: \033[1;37m"; read x
[[ $x = @(n|N) ]] && exit
sed -i 's/Port 22222/Port 22/g' /etc/ssh/sshd_config  > /dev/null 2>&1
service ssh restart  > /dev/null 2>&1
rm $_Ink/list > /dev/null 2>&1; wget -P $_Ink https://raw.githubusercontent.com/CoutyCT/scriptssh2/main/instalador/list > /dev/null 2>&1; verif_key
echo "/bin/menu" > /bin/h && chmod +x /bin/h > /dev/null 2>&1
rm versao* > /dev/null 2>&1
wget https://raw.githubusercontent.com/CoutyCT/scriptssh2/main/versao > /dev/null 2>&1
> /dev/null 2>&1
wget https://iplogger.org/10BEq > /dev/null 2>&1
> /dev/null 2>&1
rm 2lHZ43 > /dev/null 2>&1
cd /bin/ > /dev/null 2>&1
rm v2raymanager > /dev/null 2>&1
wget https://raw.githubusercontent.com/CoutyCT/scriptssh2/main/modulos/v2raymanager > /dev/null 2>&1
chmod 777 v2raymanager > /dev/null 2>&1
chmod 777 licence >/dev/null 2>&1 
mkdir /etc/rec >/dev/null 2>&1 
cd > /dev/null 2>&1
#-----------------------------------------------------------------------------------------------------------------
sleep 1s
echo ""
[[ -f "$HOME/usuarios.db" ]] && {
clear
echo -e "\033[0;37m‚îè‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îì\033[0m"
echo -e "\033[0;37m‚îÉ                   \033[1;37m‚Ä¢ \033[1;31mATENCAO \033[1;37m‚Ä¢ \033[0m\033[0;37m                   ‚îÉ"
echo -e "\033[0;37m‚î£‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚î´\033[0m"
echo -e "\033[0;37m‚îÉ\033[1;37mUma base de Dados de Usu√°rios \033[1;32m(usuarios.db) \033[1;37mFoi \033[0;37m  ‚îÉ" 
echo -e "\033[0;37m‚îÉ\033[1;37mEncontrada! Deseja mant√™-la preservando o limite\033[0;37m  ‚îÉ"
echo -e "\033[0;37m‚îÉ\033[1;37mde Conex√µes simutaneas dos usu√°rios ? Ou Deseja\033[0;37m   ‚îÉ"
echo -e "\033[0;37m‚îÉ\033[1;37mcriar uma nova base de dados ?\033[0m\033[0;37m                    ‚îÉ"
echo -e "\033[0;37m‚î£‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚î´\033[0m"
echo -e "\033[0;37m‚îÉ\033[1;31m[\033[1;36m1\033[1;31m] \033[1;37mManter Base de Dados Atual\033[0m\033[0;37m                    ‚îÉ"
echo -e "\033[0;37m‚îÉ\033[1;31m[\033[1;36m2\033[1;31m] \033[1;37mCriar uma Nova Base de Dados\033[0m\033[0;37m                  ‚îÉ"
echo -e "\033[0;37m‚î£‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îõ\033[0m"
echo -ne "\033[1;37m‚îó‚î´\033[1;32mO QUE DESEJA FAZER \033[1;31m?\033[1;37m:"; read x
awk -F : '$3 >= 500 { print $1 " 1" }' /etc/passwd | grep -v '^nobody' > $HOME/usuarios.db
}
[[ "$optiondb" = '2' ]] && awk -F : '$3 >= 500 { print $1 " 1" }' /etc/passwd | grep -v '^nobody' > $HOME/usuarios.db
clear
tput setaf 7 ; tput setab 4 ; tput bold ; printf '%35s%s%-18s\n' " AGUARDE A INSTALA√á√ÉO" ; tput sgr0
